{
  "name": "global-crm-backend",
  "version": "1.0.0",
  "description": "Backend for Global CRM system",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "build:render": "tsc -p tsconfig.render.json",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts",
    "db:setup": "ts-node src/database/init-db.ts",
    "db:init": "ts-node src/database/init-db.ts",
    "stores:init": "ts-node src/init-stores.ts",
    "cashflow:init": "node src/init-cashflow-tables.js",
    "test": "jest"
  },
  "keywords": [
    "crm",
    "api",
    "express",
    "typescript",
    "mysql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cron": "^2.4.3",
    "@types/multer": "^1.4.12",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^4.1.0",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.14.1",
    "pg-format": "^1.0.4",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.11",
    "@types/pg-format": "^1.0.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
